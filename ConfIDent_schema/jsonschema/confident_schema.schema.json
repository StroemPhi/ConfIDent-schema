{
   "$defs": {
      "AcademicEvent": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "academic_fields": {
               "description": "A relation obtaining between a obi:'planned process' and an academic field descriptor that is used to describe the scientific subject of the planned process according to some controlled vocabulary or thesaurus.",
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "alternative_ids": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "at_location": {
               "$ref": "#/$defs/Location"
            },
            "contact": {
               "type": "string"
            },
            "deadlines": {
               "items": {
                  "$ref": "#/$defs/Deadline"
               },
               "type": "array"
            },
            "denoted_by": {
               "$ref": "#/$defs/ProcessName"
            },
            "end_date": {
               "description": "A data property to provide the planned end date of an academic event in the xsd:dateTimeStamp format.",
               "format": "date-time",
               "type": "string"
            },
            "event_mode": {
               "$ref": "#/$defs/EventMode",
               "description": "The event mode describes whether the event is a physical, virtual or hybrid event."
            },
            "event_status": {
               "$ref": "#/$defs/EventStatus"
            },
            "event_type": {
               "$ref": "#/$defs/EventType",
               "description": "The sociocultural event format which is specified by the organizers, e.g. conference. Possible values are defined in the EventType enum, which uses the named individuals defined in AEON."
            },
            "id": {
               "type": "string"
            },
            "in_series": {
               "description": "The relation used to provide the series of which an AcademicEvent is a part.",
               "type": "string"
            },
            "logo": {
               "type": "string"
            },
            "metrics": {
               "items": {
                  "$ref": "#/$defs/Metric"
               },
               "type": "array"
            },
            "ordinal": {
               "description": "The ordnial number of an academic event within its series.",
               "type": "integer"
            },
            "organizers": {
               "items": {
                  "$ref": "#/$defs/Organizer"
               },
               "type": "array"
            },
            "outputs": {
               "description": "The published works that are related to the academic event or event series, such as proceedings and video recordings of talks.",
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "related_to": {
               "items": {
                  "$ref": "#/$defs/ProcessRelation"
               },
               "type": "array"
            },
            "sponsors": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "start_date": {
               "description": "A data property to provide the planned end date of  an academic event in the xsd:dateTimeStamp format.",
               "format": "date-time",
               "type": "string"
            },
            "summary": {
               "type": "string"
            },
            "topics": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "website": {
               "type": "string"
            }
         },
         "required": [
            "id",
            "denoted_by",
            "event_status",
            "start_date",
            "end_date",
            "organizers"
         ],
         "title": "AcademicEvent",
         "type": "object"
      },
      "AcademicEventSeries": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "academic_fields": {
               "description": "A relation obtaining between a obi:'planned process' and an academic field descriptor that is used to describe the scientific subject of the planned process according to some controlled vocabulary or thesaurus.",
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "alternative_ids": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "contact": {
               "type": "string"
            },
            "denoted_by": {
               "$ref": "#/$defs/ProcessName"
            },
            "end_date": {
               "description": "Similar to the start_date description but only for the end of an academic event or series.",
               "format": "date-time",
               "type": "string"
            },
            "id": {
               "type": "string"
            },
            "logo": {
               "type": "string"
            },
            "metrics": {
               "items": {
                  "$ref": "#/$defs/Metric"
               },
               "type": "array"
            },
            "organizers": {
               "items": {
                  "$ref": "#/$defs/Organizer"
               },
               "type": "array"
            },
            "outputs": {
               "description": "The published works that are related to the academic event or event series, such as proceedings and video recordings of talks.",
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "related_to": {
               "items": {
                  "$ref": "#/$defs/ProcessRelation"
               },
               "type": "array"
            },
            "series_of": {
               "description": "The relation used to provide the events belonging to an AcademicEventSeries.",
               "items": {
                  "$ref": "#/$defs/AcademicEvent"
               },
               "type": "array"
            },
            "sponsors": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "start_date": {
               "description": "The start date of an academic event or the start date of an academic event series. Wheres the latter will in reality most likely be the start date of the first event of this series, unless there is some other source from which it is possible to derive the date of the inception of the series.",
               "format": "date-time",
               "type": "string"
            },
            "summary": {
               "type": "string"
            },
            "topics": {
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "website": {
               "type": "string"
            }
         },
         "required": [
            "id",
            "denoted_by",
            "organizers"
         ],
         "title": "AcademicEventSeries",
         "type": "object"
      },
      "City": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "id": {
               "type": "string"
            },
            "name": {
               "type": "string"
            }
         },
         "required": [
            "id"
         ],
         "title": "City",
         "type": "object"
      },
      "ConfIDentRecord": {
         "additionalProperties": false,
         "description": "This is a container to be able to bundle academic event and series data in one data file (e.g. YAML or JSON).",
         "properties": {
            "events": {
               "description": "An attribute that specifies a list of academic events within this container.",
               "items": {
                  "$ref": "#/$defs/AcademicEvent"
               },
               "type": "array"
            },
            "series": {
               "description": "An attribute that specifies a list of academic event series within this container.",
               "items": {
                  "$ref": "#/$defs/AcademicEventSeries"
               },
               "type": "array"
            }
         },
         "required": [],
         "title": "ConfIDentRecord",
         "type": "object"
      },
      "Contributor": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "persons": {
               "items": {
                  "$ref": "#/$defs/Person"
               },
               "type": "array"
            },
            "type": {
               "$ref": "#/$defs/ContributorType"
            }
         },
         "required": [],
         "title": "Contributor",
         "type": "object"
      },
      "ContributorType": {
         "description": "",
         "enum": [
            "presenter",
            "keynote speaker",
            "moderator",
            "attendee"
         ],
         "title": "ContributorType",
         "type": "string"
      },
      "Country": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "id": {
               "type": "string"
            },
            "name": {
               "type": "string"
            }
         },
         "required": [
            "id"
         ],
         "title": "Country",
         "type": "object"
      },
      "Deadline": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "deadline_date": {
               "format": "date-time",
               "type": "string"
            },
            "deadline_other": {
               "type": "string"
            },
            "type": {
               "$ref": "#/$defs/DeadlineType"
            }
         },
         "required": [
            "deadline_date",
            "type"
         ],
         "title": "Deadline",
         "type": "object"
      },
      "DeadlineType": {
         "description": "",
         "enum": [
            "submission deadline",
            "notification deadline",
            "abstract deadline",
            "camera-ready deadline",
            "demo deadline",
            "paper deadline",
            "poster deadline",
            "tutorial deadline",
            "workshop deadline"
         ],
         "title": "DeadlineType",
         "type": "string"
      },
      "EventMode": {
         "description": "",
         "enum": [
            "on site",
            "online",
            "hybrid"
         ],
         "title": "EventMode",
         "type": "string"
      },
      "EventStatus": {
         "description": "",
         "enum": [
            "as scheduled",
            "postponed",
            "delayed",
            "canceled",
            "planned"
         ],
         "title": "EventStatus",
         "type": "string"
      },
      "EventType": {
         "additionalProperties": false,
         "description": "An event type is a plan specification that specifies the sociocultural format or type of the academic event by providing details on the objectives, conditions and actions needed in its realization.",
         "properties": {
            "event_type_iri": {
               "$ref": "#/$defs/EventTypeValue"
            },
            "event_type_other": {
               "type": "string"
            }
         },
         "required": [
            "event_type_iri"
         ],
         "title": "EventType",
         "type": "object"
      },
      "EventTypeValue": {
         "description": "The following definitions attempt to provide descriptions to distinguish different academic event formats, even though the definitions are not always clearly distinguishable in their everyday use and are often used synonymously. According to the mapping to [AEON](https://github.com/tibonto/aeon), each permissible value on this list represents an AEON named individual",
         "enum": [
            "colloquium",
            "conference",
            "congress",
            "event session",
            "poster session",
            "event talk",
            "keynote speech",
            "event track",
            "forum",
            "hackathon",
            "seminar",
            "symposium",
            "tutorial",
            "workshop",
            "other event type"
         ],
         "title": "EventTypeValue",
         "type": "string"
      },
      "Location": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "city": {
               "type": "string"
            },
            "country": {
               "type": "string"
            },
            "lattitude": {
               "type": "number"
            },
            "longitude": {
               "type": "number"
            },
            "meeting_url": {
               "type": "string"
            },
            "region": {
               "type": "string"
            },
            "venue": {
               "type": "string"
            }
         },
         "required": [],
         "title": "Location",
         "type": "object"
      },
      "Metric": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "int_value": {
               "type": "integer"
            },
            "other_metric": {
               "type": "string"
            },
            "rate_value": {
               "type": "number"
            },
            "str_value": {
               "type": "string"
            },
            "truth_value": {
               "type": "boolean"
            },
            "type": {
               "$ref": "#/$defs/MetricType"
            }
         },
         "required": [],
         "title": "Metric",
         "type": "object"
      },
      "MetricType": {
         "description": "",
         "enum": [
            "submitted papers",
            "accepted papers",
            "accepted short papers",
            "number of attendees",
            "number of tracks",
            "has childcare options",
            "is accessible"
         ],
         "title": "MetricType",
         "type": "string"
      },
